{
  "name": "bilten-backend",
  "version": "1.0.0",
  "description": "Bilten Event Management Platform Backend API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
